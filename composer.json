  "description": "This is a library for using containers of objects. This structure provides a few neat advantages.",
  "license": "Apache",
  "version": "0.1",
  "authors": [
    {
      "name": "Iosif Vigh",
      "email": "vighiosif@gmail.com"
    }
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpro/grumphp": "dev-master",
    "sensiolabs/security-checker": "~3.0@dev",
    "squizlabs/php_codesniffer": "~2.0@dev",
    "phpmd/phpmd": "dev-master",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "VighIosif\\ObjectContainers\\": "src/",
      "VighIosif\\ObjectContainers\\Abstracts\\": "src/abstracts/",
      "VighIosif\\ObjectContainers\\Classes\\": "src/classes/",
      "VighIosif\\ObjectContainers\\Interfaces\\": "src/interfaces/",
      "VighIosif\\ObjectContainers\\Traits\\": "src/traits/",
      "VighIosif\\ObjectContainers\\Tests\\": "tests/"
